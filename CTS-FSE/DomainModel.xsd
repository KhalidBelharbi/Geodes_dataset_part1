<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:complexType name="image" >
      <xs:sequence>
        <xs:element name="imageid" type="xs:long" />
        <xs:element name="imageurl" type="xs:string" />
        <xs:element name="GeneratedName" type="album" nillable="true" minOccurs="0" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="album" >
      <xs:sequence>
        <xs:element name="albumid" type="xs:long" />
        <xs:element name="albumname" type="xs:string" />
        <xs:element name="crtedon" type="xs:anyType" />
        <xs:element name="GeneratedName" type="image" nillable="true" minOccurs="0" />
      </xs:sequence>
   </xs:complexType>
</xs:schema> 