      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="user" >
      <xs:complexContent>
       <xs:extension base="grantedauthority>">
         <xs:sequence>
          <xs:element name="" type="xs:anyType" />
          <xs:element name="uid" type="xs:long" />
          <xs:element name="phone" type="xs:string" />
          <xs:element name="thirdpartname" type="xs:string" />
          <xs:element name="nickname" type="xs:string" />
          <xs:element name="" type="xs:anyType" />
          <xs:element name="headiconurl" type="xs:string" />
          <xs:element name="password" type="xs:string" />
          <xs:element name="lastloginip" type="xs:string" />
          <xs:element name="lastlogintime" type="xs:dateTime" />
          <xs:element name="memberstatus" type="xs:anyType" />
          <xs:element name="roles" type="userauthrole" nillable="true" minOccurs="0" maxOccurs="unbounded" />
         </xs:sequence>
       </xs:extension>
      </xs:complexContent>
   </xs:complexType>
