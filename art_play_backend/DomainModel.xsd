        <xs:element name="ar" type="image" nillable="true" minOccurs="0" />
        <xs:element name="arbindtime" type="xs:dateTime" />
        <xs:element name="//包含信息resourceitems" type="resourceitem" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="beacon" >
      <xs:sequence>
        <xs:element name="//外界请不要使用该id操作数据id" type="xs:long" />
        <xs:element name="showid" type="xs:long" />
        <xs:element name="basicinfo" type="installbeacon" nillable="true" minOccurs="0" />
        <xs:element name="available" type="xs:anyType" />
        <xs:element name="GeneratedName" type="xs:anyType" />
        <xs:element name="//@notnullname" type="xs:string" />
        <xs:element name="GeneratedName" type="showmap" nillable="true" minOccurs="0" />
        <xs:element name="positionwidth" type="xs:anyType" />
        <xs:element name="positionheight" type="xs:anyType" />
        <xs:element name="//绑定信息//@onetoone(targetentity" type="xs:long" />
        <xs:element name="//建议：获取到beacon信息后需要将此数据清空" type="resourcegroup" nillable="true" minOccurs="0" />
        <xs:element name="//全部为0iconstatus" type="xs:anyType" />
        <xs:element name="uptime" type="xs:dateTime" />
      </xs:sequence>
