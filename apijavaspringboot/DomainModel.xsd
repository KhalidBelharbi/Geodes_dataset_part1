      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="rol" >
      <xs:sequence>
        <xs:element name="idrol" type="xs:anyType" />
        <xs:element name="nombre" type="xs:string" />
        <xs:element name="descripcion" type="xs:string" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="turnocomida" >
      <xs:sequence>
        <xs:element name="idturnocomida" type="xs:anyType" />
        <xs:element name="nombre" type="xs:string" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="alimento" >
      <xs:sequence>
        <xs:element name="idalimento" type="xs:anyType" />
        <xs:element name="nombre" type="xs:string" />
        <xs:element name="cantidad" type="xs:string" />
