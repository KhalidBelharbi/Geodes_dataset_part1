        <xs:element name="lstdias*/lstdetalleplanes" type="detalleplan" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="detalleplan" >
      <xs:sequence>
        <xs:element name="iddetalle" type="xs:anyType" />
        <xs:element name="GeneratedName" type="plan" nillable="true" minOccurs="0" />
        <xs:element name="GeneratedName" type="alimento" nillable="true" minOccurs="0" />
        <xs:element name="GeneratedName" type="turnocomida" nillable="true" minOccurs="0" />
        <xs:element name="GeneratedName" type="dias" nillable="true" minOccurs="0" />
      </xs:sequence>
   </xs:complexType>
</xs:schema> 