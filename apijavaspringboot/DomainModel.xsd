        <xs:element name="estatura" type="xs:double" />
        <xs:element name="cabeza" type="xs:double" />
        <xs:element name="observacion" type="xs:string" />
        <xs:element name="//@jsonignore" type="paciente" nillable="true" minOccurs="0" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="prediccion" >
      <xs:sequence>
        <xs:element name="idprediccion" type="xs:anyType" />
