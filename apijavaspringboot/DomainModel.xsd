        <xs:element name="chocdf" type="xs:string" />
        <xs:element name="choavl" type="xs:string" />
        <xs:element name="fib" type="xs:string" />
        <xs:element name="fibtg" type="xs:string" />
        <xs:element name="ash" type="xs:string" />
        <xs:element name="ca" type="xs:string" />
        <xs:element name="p" type="xs:string" />
        <xs:element name="zn" type="xs:string" />
        <xs:element name="fe" type="xs:string" />
        <xs:element name="beta" type="xs:string" />
        <xs:element name="retinol" type="xs:string" />
        <xs:element name="vita" type="xs:string" />
        <xs:element name="thia" type="xs:string" />
        <xs:element name="ribf" type="xs:string" />
        <xs:element name="nia" type="xs:string" />
        <xs:element name="vitc" type="xs:string" />
        <xs:element name="GeneratedName" type="grupo" nillable="true" minOccurs="0" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="usuario" >
      <xs:sequence>
        <xs:element name="idusuario" type="xs:anyType" />
        <xs:element name="/*	*@apimodelproperty(notes" type="xs:string" />
        <xs:element name="password" type="xs:string" />
        <xs:element name="/*	*@email	*	*@column(name" type="xs:anyType" />
        <xs:element name="/*	*@column(name" type="rol" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="dalimentoturnocomida" >
      <xs:sequence>
        <xs:element name="GeneratedName" type="alimento" nillable="true" minOccurs="0" />
        <xs:element name="GeneratedName" type="turnocomida" nillable="true" minOccurs="0" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="dosis" >
      <xs:sequence>
        <xs:element name="iddosis" type="xs:anyType" />
