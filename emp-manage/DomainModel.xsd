      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="deptmanager" >
      <xs:sequence>
        <xs:element name="empno" type="xs:long" />
        <xs:element name="deptno" type="xs:string" />
        <xs:element name="from" type="xs:dateTime" />
        <xs:element name="to" type="xs:dateTime" />
      </xs:sequence>
   </xs:complexType>
